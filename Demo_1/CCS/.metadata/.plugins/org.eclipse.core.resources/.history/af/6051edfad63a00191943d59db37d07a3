/*
 * main.c
 */
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#define length 10
#include "max.h"
#include "abs.h"
#include "norm.h"
#include "correlation.h"

#define ATTENTE 0
#define SIGNAL_PAS_OK 1
#define SIGNAL_OK 2


double t[length];
int i;
double s_ref[length];


int main(void) {


    for (i = 0; i < length; i++)
    {
        s_ref[i] = sin(i*3.14 / 10);
    }

    double out[2 * length - 1] = {0};
    correlation(s_ref, s_ref, length,out);
    printf("Le resultat de la correlation est \n");
    printf("[ %f", out[0]);


    i = 1;
    for (; i < 2 * length - 1; i++)
    {
        printf(", %f",out[i]);
    }
    printf("]");

	
	return 0;
}
